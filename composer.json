{
  "name": "bfitech/zapchupload",
  "description": "Server-side chunk upload.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rendra Suroso",
      "email": "rs@cogsci.bandungfe.net"
    }
  ],
  "require": {
    "php": ">=5.5 || ~7.0"
  },
  "suggest": {
    "bfitech/zapcore": "Specifically-designed for this router although there's no explicit dependency."
  },
  "require-dev": {
    "bfitech/zapcore": "~1.1",
    "bfitech/zaplate": "~1",
    "phpunit/phpunit": "4.8.*",
    "guzzlehttp/guzzle": "6.*"
  },
  "autoload": {
    "psr-4": {
      "BFITech\\ZapChupload\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BFITech\\ZapCoreDev\\": ""
    }
  }
}
